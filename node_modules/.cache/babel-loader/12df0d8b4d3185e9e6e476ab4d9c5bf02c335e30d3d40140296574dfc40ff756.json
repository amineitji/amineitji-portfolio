:class=\"'category-' + project.category\">\n                  {{ project.categoryLabel }}\n                </div>\n              </div>\n              <div class=\"project-preview-content\">\n                <h4 class=\"project-preview-title\">{{ project.title }}</h4>\n                <p class=\"project-preview-description\">{{ project.description }}</p>\n                <div class=\"project-preview-tech\">\n                  <span v-for=\"tech in project.technologies\" :key=\"tech\" class=\"tech-tag-small\">\n                    {{ tech }}\n                  </span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      recentProjects: [\n        {\n          id: 1,\n          title: \"Analyse de matchs de football par IA\",\n          description: \"Système d'analyse automatique utilisant YOLO et computer vision\",\n          image: \"https://via.placeholder.com/300x200/3b82f6/white?text=Football+AI\",\n          category: \"perso\",\n          categoryLabel: \"Personnel\",\n          technologies: [\"Python\", \"YOLO\", \"OpenCV\", \"Machine Learning\"]\n        },\n        {\n          id: 2,\n          title: \"DiagERP - Plateforme E-commerce\",\n          description: \"Plateforme complète pour rapports techniques immobiliers\",\n          image: \"https://via.placeholder.com/300x200/10b981/white?text=DiagERP\",\n          category: \"pro\",\n          categoryLabel: \"Professionnel\",\n          technologies: [\"Django\", \"Vue.js\", \"AWS\", \"PostgreSQL\"]\n        },\n        {\n          id: 3,\n          title: \"Web Scraping Football Analytics\",\n          description: \"Extraction et visualisation de données WhoScored\",\n          image: \"https://via.placeholder.com/300x200/8b5cf6/white?text=Scraping\",\n          category: \"perso\",\n          categoryLabel: \"Personnel\",