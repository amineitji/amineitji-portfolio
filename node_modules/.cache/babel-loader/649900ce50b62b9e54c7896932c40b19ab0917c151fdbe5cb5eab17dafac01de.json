,"_hoisted_2","_hoisted_3","_hoisted_4","type","onClick","_cache","args","$options","toggleNavbar","_normalizeClass","$data","isCollapsed","_hoisted_8","_hoisted_9","_Fragment","_renderList","navItems","item","key","name","_createVNode","_component_router_link","to","link","isActive","closeMenu","default","_withCtx","_hoisted_10","_toDisplayString","_hoisted_11","_"],"sources":["/home/amine/Bureau/PRO/portfolio/amineitji-portfolio/src/components/SiteHeader.vue"],"sourcesContent":["<template>\n  <header class=\"site-header\">\n    <!-- Fond animé avec effet de particules -->\n    <div class=\"header-bg\">\n      <div class=\"header-glow\"></div>\n    </div>\n    \n    <!-- Logo et navigation -->\n    <nav class=\"navbar\">\n      <!-- Logo avec effet néon -->\n      <div class=\"logo-container\">\n        <div class=\"logo\">\n          <span class=\"logo-text\">A<span class=\"logo-dot\">.</span></span>\n        </div>\n      </div>\n      \n      <!-- Bouton du menu mobile -->\n      <button class=\"navbar-toggler\" type=\"button\" @click=\"toggleNavbar\">\n        <div class=\"menu-icon\" :class=\"{ 'is-active': isCollapsed }\">\n          <span></span>\n          <span></span>\n          <span></span>\n        </div>\n      </button>\n      \n      <!-- Menu de navigation -->\n      <div :class=\"['navbar-collapse', { 'show': isCollapsed }]\">\n        <ul class=\"navbar-nav\">\n          <li class=\"nav-item\" v-for=\"item in navItems\" :key=\"item.name\">\n            <router-link\n              :to=\"item.link\"\n              class=\"nav-link\"\n              :class=\"{ 'active': isActive(item.link) }\"\n              @click=\"closeMenu\"\n            >\n              <span class=\"link-text\">{{ item.name }}</span>\n              <span class=\"link-hover-effect\"></span>\n            </router-link>\n          </li>\n        </ul>\n      </div>\n    </nav>\n  </header>\n</template>\n\n<script>\nexport default {\n  name: 'SiteHeader',\n  data() {\n    return {\n      isCollapsed: false,\n      navItems: [\n        { name: 'Accueil', link: '/' },\n        { name: 'À propos de moi', link: '/profil' },\n        { name: 'Projets Universitaires', link: '/projets-universitaires' },\n        { name: 'Projets Personnels', link: '/projets-personnels' },\n        { name: 'Commandez un site vitrine', link: '/commandez' },\n      ],\n    };\n  },\n  methods: {\n    toggleNavbar() {\n      this.isCollapsed = !this.isCollapsed;\n      \n      // Ajout d'une classe au body pour empêcher le défilement\n      if (this.isCollapsed) {\n        document.body.classList.add('menu-open');\n      } else {\n        document.body.classList.remove('menu-open');\n      }\n    },\n    isActive(link) {\n      return this.$route.path === link;\n    },\n    closeMenu() {\n      if (this.isCollapsed) {\n        this.isCollapsed = false;\n        document.body.classList.remove('menu-open');\n      }\n    }\n  },\n  // S'assurer que la classe menu-open est retirée lors de la navigation\n  watch: {\n    $route() {\n      this.isCollapsed = false;\n      document.body.classList.remove('menu-open');\n    }\n  },\n  // S'assurer que tout est nettoyé lors de la destruction du composant\n  beforeUnmount() {\n    document.body.classList.remove('menu-open');\n  }\n};\n</script>\n\n<style scoped>\n/* Variables */\n:root {\n  --neon-green: #42b883;\n  --neon-blue: #00d8ff;\n  --neon-purple: #bd34fe;\n  --dark-bg: #0a0b13;\n  --card-bg: #12141f;\n  --text-primary: #f0f0f0;\n  --text-secondary: #b4b4b8;\n  --border-color: #2a2d36;\n  --header-height: 70px;\n  --nav-shadow: 0 5px 20px rgba(0, 0, 0, 0.2);\n}\n\n/* Base Header Styles */\n.site-header {\n  position: sticky;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: var(--header-height);\n  z-index: 1000;\n  backdrop-filter: blur(15px);\n  transition: all 0.4s ease;\n}\n\n/* Fond animé avec glow */\n.header-bg {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  background-color: rgba(10, 11, 19, 0.8);\n  overflow: hidden;\n  z-index: -1;\n}\n\n.header-glow {\n  position: absolute;\n  top: -50%;\n  left: -5%;\n  width: 110%;\n  height: 200%;\n  background: linear-gradient(90deg, \n    rgba(66, 184, 131, 0), \n    rgba(66, 184, 131, 0.03), \n    rgba(0, 216, 255, 0.05), \n    rgba(66, 184, 131, 0.03), \n    rgba(66, 184, 131, 0));\n  background-size: 400% 400%;\n  animation: gradientMove 15s ease infinite;\n  transform: skewY(-12deg);\n}\n\n@keyframes gradientMove {\n  0% { background-position: 0% 50%; }\n  50% { background-position: 100% 50%; }\n  100% { background-position: 0% 50%; }\n}\n\n/* Navbar Layout */\n.navbar {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  height: 100%;\n  padding: 0 30px;\n  max-width: 1400px;\n  margin: 0 auto;\n}\n\n/* Logo Styles */\n.logo-container {\n  display: flex;\n  align-items: center;\n  transition: transform 0.3s ease;\n}\n\n.logo-container:hover {\n  transform: scale(1.05);\n}\n\n.logo {\n  position: relative;\n  width: 40px;\n  height: 40px;\n  border-radius: 50%;\n  background: linear-gradient(135deg, var(--neon-green), var(--neon-blue));\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  box-shadow: 0 0 15px rgba(66, 184, 131, 0.5);\n  animation: pulse 4s infinite;\n}\n\n@keyframes pulse {\n  0% { box-shadow: 0 0 0 0 rgba(66, 184, 131, 0.4); }\n  70% { box-shadow: 0 0 0 10px rgba(66, 184, 131, 0); }\n  100% { box-shadow: 0 0 0 0 rgba(66, 184, 131, 0); }\n}\n\n.logo-text {\n  color: white;\n  font-weight: 700;\n  font-size: 20px;\n}\n\n.logo-dot {\n  color: rgba(255, 255, 255, 0.8);\n  animation: blinkDot 2s infinite;\n}\n\n@keyframes blinkDot {\n  0%, 100% { opacity: 1; }\n  50% { opacity: 0.3; }\n}\n\n/* Navigation Links */\n.navbar-nav {\n  display: flex;\n  list-style: none;\n  margin: 0;\n  padding: 0;\n  transition: all 0.5s ease;\n}\n\n.nav-item {\n  margin: 0 5px;\n}\n\n.nav-link {\n  position: relative;\n  display: block;\n  padding: 8px 15px;\n  color: var(--text-primary);\n  text-decoration: none;\n  font-size: 1rem;\n  font-weight: 500;\n  transition: all 0.3s ease;\n  border-radius: 8px;\n  overflow: hidden;\n}\n\n.link-text {\n  position: relative;\n  z-index: 2;\n}\n\n.link-hover-effect {\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  width: 100%;\n  height: 0%;\n  background: linear-gradient(to top, rgba(66, 184, 131, 0.1), transparent);\n  transition: height 0.3s ease;\n  z-index: 1;\n}\n\n.nav-link:hover .link-hover-effect,\n.nav-link.active .link-hover-effect {\n  height: 100%;\n}\n\n.nav-link:hover {\n  color: var(--neon-green);\n  transform: translateY(-2px);\n}\n\n.nav-link.active {\n  color: var(--neon-green);\n  font-weight: 600;\n}\n\n.nav-link.active::after {\n  content: '';\n  position: absolute;\n  bottom: 0;\n  left: 15%;\n  width: 70%;\n  height: 3px;\n  background: linear-gradient(90deg, transparent, var(--neon-green), transparent);\n  border-radius: 3px;\n  box-shadow: 0 0 8px rgba(66, 184, 131, 0.5);\n}\n\n/* Hamburger Menu Button */\n.navbar-toggler {\n  display: none;\n  background: transparent;\n  border: none;\n  cursor: pointer;\n  outline: none;\n  padding: 0;\n  z-index: 1001;\n}\n\n.menu-icon {\n  width: 30px;\n  height: 24px;\n  position: relative;\n  transform: rotate(0deg);\n  transition: 0.5s ease-in-out;\n  cursor: pointer;\n}\n\n.menu-icon span {\n  display: block;\n  position: absolute;\n  height: 3px;\n  width: 100%;\n  background: var(--neon-green);\n  border-radius: 3px;\n  opacity: 1;\n  left: 0;\n  transform: rotate(0deg);\n  transition: 0.25s ease-in-out;\n  box-shadow: 0 0 5px rgba(66, 184, 131, 0.5);\n}\n\n.menu-icon span:nth-child(1) {\n  top: 0px;\n}\n\n.menu-icon span:nth-child(2) {\n  top: 10px;\n  width: 80%;\n}\n\n.menu-icon span:nth-child(3) {\n  top: 20px;\n}\n\n.menu-icon.is-active span:nth-child(1) {\n  top: 10px;\n  transform: rotate(135deg);\n}\n\n.menu-icon.is-active span:nth-child(2) {\n  opacity: 0;\n  left: -60px;\n}\n\n.menu-icon.is-active span:nth-child(3) {\n  top: 10px;\n  transform: rotate(-135deg);\n}\n\n/* Responsive Design */\n@media (max-width: 992px) {\n  .navbar-toggler {\n    display: block;\n  }\n  \n  .navbar-collapse {\n    position: fixed;\n    top: var(--header-height);\n    left: 0;\n    width: 100%;\n    height: 0;\n    background: rgba(10, 11, 19, 0.95);\n    backdrop-filter: blur(10px);\n    overflow: hidden;\n    transition: height 0.4s cubic-bezier(0.77, 0, 0.175, 1);\n    z-index: 1000;\n  }\n  \n  .navbar-collapse.show {\n    height: calc(100vh - var(--header-height));\n    box-shadow: var(--nav-shadow);\n  }\n  \n  .navbar-nav {\n    flex-direction: column;\n    align-items: center;\n    justify-content: center;\n    height: 100%;\n    opacity: 0;\n    transform: translateY(20px);\n    transition: all 0.3s ease;\n  }\n  \n  .navbar-collapse.show .navbar-nav {\n    opacity: 1;\n    transform: translateY(0);\n    transition-delay: 0.2s;\n  }\n  \n  .nav-item {\n    margin: 10px 0;\n    width: 80%;\n    text-align: center;\n  }\n  \n  .nav-link {\n    padding: 15px;\n    font-size: 1.2rem;\n  }\n  \n  .nav-link.active::after {\n    width: 50%;\n    left: 25%;\n  }\n  \n  /* État global du body quand le menu est ouvert */\n  :global(body.menu-open) {\n    overflow: hidden;\n  }\n}\n\n/* Animation d'entrée pour les éléments du menu */\n@keyframes fadeInNavItem {\n  from {\n    opacity: 0;\n    transform: translateY(20px);\n  }\n  to {\n    opacity: 1;\n    transform: translateY(0);\n  }\n}\n\n.navbar-collapse.show .nav-item {\n  animation: fadeInNavItem 0.5s forwards;\n}\n\n.navbar-collapse.show .nav-item:nth-child(1) {\n  animation-delay: 0.1s;\n}\n\n.navbar-collapse.show .nav-item:nth-child(2) {\n  animation-delay: 0.2s;\n}\n\n.navbar-collapse.show .nav-item:nth-child(3) {\n  animation-delay: 0.3s;\n}\n\n.navbar-collapse.show .nav-item:nth-child(4) {\n  animation-delay: 0.4s;\n}\n\n.navbar-collapse.show .nav-item:nth-child(5) {\n  animation-delay: 0.5s;\n}\n</style>"],"ma