<div class=\"profile-sidebar\">\n          <!-- Navigation rapide -->\n          <nav class=\"quick-nav\">\n            <router-link to=\"/\" class=\"nav-item active\">\n              <i class=\"fas fa-home\"></i>\n              <span>Accueil</span>\n            </router-link>\n            <router-link to=\"/projets\" class=\"nav-item\">\n              <i class=\"fas fa-code\"></i>\n              <span>Projets</span>\n            </router-link>\n            <router-link to=\"/services\" class=\"nav-item\">\n              <i class=\"fas fa-briefcase\"></i>\n              <span>Services</span>\n            </router-link>\n            <router-link to=\"/contact\" class=\"nav-item\">\n              <i class=\"fas fa-envelope\"></i>\n              <span>Contact</span>\n            </router-link>\n          </nav>\n          \n          <!-- CTA section -->\n          <div class=\"cta-section\">\n            <h3>Prêt à collaborer ?</h3>\n            <p>Discutons de votre projet</p>\n            <a href=\"mailto:amineitji@gmail.com\" class=\"btn btn-primary\">\n              Démarrer un projet\n            </a>\n          </div>\n        </div>\n      </aside>\n      \n      <!-- Contenu principal -->\n      <main class=\"content-column\">\n        <!-- Header du profil -->\n        <div class=\"profile-header\">\n          <div class=\"cover-photo\"></div>\n          <div class=\"profile-info\">\n            <div class=\"avatar-section\">\n              <img \n                src=\"https://media.licdn.com/dms/image/v2/D4E03AQE3PrNtqhlw7w/profile-displayphoto-shrink_200_200/profile-displayphoto-shrink_200_200/0/1721847794814?e=2147483647&v=beta&t=-EYtas69BGrXW4KCc708xDpqTbBRHuMtL3f6Oknw4CI\"\n                alt=\"Amine ITJI\"\n                class=\"profile-avatar\"\n              />\n              <div class=\"status-indicator\"></div>\n            </div>\n            <div class=\"profile-details\">\n              <h1 class=\"profile-name\">Amine ITJI</h1>\n              <p class=\"profile-handle\">@amineitji</p>\n              <p class=\"profile-title\">Master 2 Intelligence Artificielle • Développeur Full-Stack</p>\n              \n              <div class=\"profile-meta\">\n                <div class=\"meta-item\">\n                  <i class=\"fas fa-map-marker-alt\"></i>\n                  <span>Lyon, France</span>\n                </div>\n                <div class=\"meta-item\">\n                  <i class=\"fas fa-calendar-alt\"></i>\n                  <span>Actif depuis 2020</span>\n                </div>\n                <div class=\"meta-item\">\n                  <i class=\"fas fa-link\"></i>\n                  <a href=\"https://linkedin.com/in/amine-itji-5a8696268\" target=\"_blank\">linkedin.com/in/amine-itji</a>\n                </div>\n              </div>\n              \n              <div class=\"profile-stats\">\n                <div class=\"stat\">\n                  <span class=\"stat-number\">15+</span>\n                  <span class=\"stat-label\">Projets</span>\n                </div>\n                <div class=\"stat\">\n                  <span class=\"stat-number\">50+</span>\n                  <span class=\"stat-label\">Technologies</span>\n                </div>\n                <div class=\"stat\">\n                  <span class=\"stat-number\">3</span>\n                  <span class=\"stat-label\">Ans d'expérience</span>\n                </div>\n              </div>\n            </div>\n            \n            <div class=\"profile-actions\">\n              <a href=\"mailto:amineitji@gmail.com\" class=\"btn btn-primary\">\n                <i class=\"fas fa-envelope\"></i>\n                Me contacter\n              </a>\n              <button class=\"btn btn-secondary\">\n                <i class=\"fas fa-download\"></i>\n                CV PDF\n              </button>\n            </div>\n          </div>\n        </div>\n        \n        <!-- Navigation des onglets -->\n        <div class=\"profile-tabs\">\n          <button class=\"tab\" :class=\"{ active: activeTab === 'presentation' }\" @click=\"activeTab = 'presentation'\">\n            Présentation\n          </button>\n          <button class=\"tab\" :class=\"{ active: activeTab === 'projects' }\" @click=\"activeTab = 'projects'\">\n            Projets récents\n          </button>\n          <button class=\"tab\" :class=\"{ active: activeTab === 'skills' }\" @click=\"activeTab = 'skills'\">\n            Compétences\n          </button>\n        </div>\n        \n        <!-- Contenu principal -->\n        <div class=\"profile-content\">\n          <!-- Bio -->\n          <div v-if=\"activeTab === 'presentation'\" class=\"tab-content fade-in\">\n            <div class=\"bio-section card\">\n              <h3>À propos</h3>\n              <div class=\"bio-content\">\n                <p>\n                  <strong>Étudiant en Master 2 Intelligence Artificielle</strong> à l'Université Claude Bernard Lyon 1, \n                  spécialisé en machine learning, computer vision et data science.\n                </p>\n                <p>\n                  <strong>Développeur Freelance</strong> expérimenté dans la création de sites vitrine modernes \n                  et d'applications web complexes avec Django/Vue.js. Expert en solutions de web scraping \n                  et automatisation de processus.\n                </p>\n                <p>\n                  <strong>Recherche active</strong> d'un stage de fin d'études (6 mois) avec possibilité \n                  de pré-embauche dans les domaines de l'IA, du développement web ou de la data science.\n                </p>\n              </div>\n              \n              <!-- Status badges -->\n              <div class=\"status-badges\">\n                <div class=\"status-badge searching\">\n                  <i class=\"fas fa-search\"></i>\n                  <span>Recherche stage de fin d'études</span>\n                </div>\n                <div class=\"status-badge available\">\n                  <i class=\"fas fa-check-circle\"></i>\n                  <span>Disponible en freelance</span>\n                </div>\n              </div>\n            </div>\n            \n            <!-- Parcours -->\n            <div class=\"experience-section card\">\n              <h3>Parcours récent</h3>\n              <div class=\"timeline\">\n                <div class=\"timeline-item\">\n                  <div class=\"timeline-date\">Janvier - Mars 2025</div>\n                  <div class=\"timeline-content\">\n                    <h4>Stage - Digitalisation des inspections</h4>\n                    <p class=\"company\">ALSTOM - Villeurbanne</p>\n                    <p class=\"description\">Mise en place de formulaires d'inspections sur KIZEO avec méthodologie Agile.</p>\n                  </div>\n                </div>\n                <div class=\"timeline-item\">\n                  <div class=\"timeline-date\">Mai - Juin 2023</div>\n                  <div class=\"timeline-content\">\n                    <h4>Stage - Développeur Fullstack Django/VueJs</h4>\n                    <p class=\"company\">ANEQQIS - Paris</p>\n                    <p class=\"description\">Création autonome d'un site de génération et commercialisation de rapports techniques immobiliers.</p>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          \n          <!-- Projets récents -->\n          <div v-if=\"activeTab === 'projects'\" class=\"tab-content fade-in\">\n            <div class=\"recent-projects-section card\">\n              <div class=\"section-header\">\n                <h3>Projets récents</h3>\n                <router-link to=\"/projets\" class=\"view-all-link\">\n                  Voir tous les projets\n                  <i class=\"fas fa-arrow-right\"></i>\n                </router-link>\n              </div>\n              \n              <div class=\"projects-grid\">\n                <div class=\"project-card\" v-for=\"project in recentProjects\" :key=\"project.id\">\n                  <div class=\"project-image\">\n                    <img :src=\"project.image\" :alt=\"project.title\" />\n                    <div class=\"project-category\" :class=\"'category-' + project.category\">\n                      {{ project.categoryLabel }}\n                    </div>\n                  </div>\n                  <div class=\"project-content\">\n                    <h4 class=\"project-title\">{{ project.title }}</h4>\n                    <p class=\"project-description\">{{ project.description }}</p>\n                    <div class=\"project-tech\">\n                      <span v-for=\"tech in project.technologies\" :key=\"tech\" class=\"tech-tag-small\">\n                        {{ tech }}\n                      </span>\n                    </div>\n                    <div class=\"project-links\">\n                      <a v-if=\"project.github\" :href=\"project.github\" target=\"_blank\" class=\"project-link\">\n                        <i class=\"fab fa-github\"></i>\n                        Code\n                      </a>\n                      <a v-if=\"project.demo\" :href=\"project.demo\" target=\"_blank\" class=\"project-link\">\n                        <i class=\"fas fa-external-link-alt\"></i>\n                        Démo\n                      </a>\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n          \n          <!-- Compétences -->\n          <div v-if=\"activeTab === 'skills'\" class=\"tab-content fade-in\">\n            <div class=\"tech-stack-section card\">\n              <h3>Stack technique</h3>\n              <div class=\"tech-categories\">\n                <div class=\"tech-category\">\n                  <h4>Intelligence Artificielle</h4>\n                  <div class=\"tech-tags\">\n                    <span class=\"tech-tag expert\">Python</span>\n                    <span class=\"tech-tag expert\">Machine Learning</span>\n                    <span class=\"tech-tag expert\">Computer Vision</span>\n                    <span class=\"tech-tag advanced\">TensorFlow</span>\n                    <span class=\"tech-tag advanced\">OpenCV</span>\n                    <span class=\"tech-tag advanced\">Scikit-learn</span>\n                  </div>\n                </div>\n                \n                <div class=\"tech-category\">\n                  <h4>Développement Web</h4>\n                  <div class=\"tech-tags\">\n                    <span class=\"tech-tag expert\">Django</span>\n                    <span class=\"tech-tag expert\">Vue.js</span>\n                    <span class=\"tech-tag expert\">JavaScript</span>\n                    <span class=\"tech-tag advanced\">PostgreSQL</span>\n                    <span class=\"tech-tag advanced\">Redis</span>\n                    <span class=\"tech-tag intermediate\">React</span>\n                  </div>\n                </div>\n                \n                <div class=\"tech-category\">\n                  <h4>DevOps & Cloud</h4>\n                  <div class=\"tech-tags\">\n                    <span class=\"tech-tag expert\">AWS</span>\n                    <span class=\"tech-tag expert\">Docker</span>\n                    <span class=\"tech-tag advanced\">GitHub Actions</span>\n                    <span class=\"tech-tag advanced\">Nginx</span>\n                    <span class=\"tech-tag intermediate\">Kubernetes</span>\n                  </div>\n                </div>\n                \n                <div class=\"tech-category\">\n                  <h4>Data & Automation</h4>\n                  <div class=\"tech-tags\">\n                    <span class=\"tech-tag expert\">Web Scraping</span>\n                    <span class=\"tech-tag expert\">Pandas</span>\n                    <span class=\"tech-tag expert\">Selenium</span>\n                    <span class=\"tech-tag advanced\">BeautifulSoup</span>\n                    <span class=\"tech-tag advanced\">Data Visualization</span>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n      </main>\n      \n      <!-- Sidebar droite -->\n      <aside class=\"sidebar-right\">\n        <!-- Trending/What's happening -->\n        <div class=\"trending-section card\">\n          <h3>Actualités Tech</h3>\n          <div class=\"trending-items\">\n            <div class=\"trending-item\">\n              <div class=\"trending-meta\">Tendance en Développement</div>\n              <div class=\"trending-title\">Vue.js 3.4</div>\n              <div class=\"trending-stats\">15.2k discussions</div>\n            </div>\n            <div class=\"trending-item\">\n              <div class=\"trending-meta\">Tendance en IA</div>\n              <div class=\"trending-title\">Machine Learning</div>\n              <div class=\"trending-stats\">8.7k discussions</div>\n            </div>\n            <div class=\"trending-item\">\n              <div class=\"trending-meta\">Tendance Tech</div>\n              <div class=\"trending-title\">Python Django</div>\n              <div class=\"trending-stats\">5.3k discussions</div>\n            </div>\n            <div class=\"trending-item\">\n              <div class=\"trending-meta\">Tendance Cloud</div>\n              <div class=\"trending-title\">AWS DevOps</div>\n              <div class=\"trending-stats\">3.1k discussions</div>\n            </div>\n          </div>\n          <a href=\"#\" class=\"show-more\">Afficher plus</a>\n        </div>\n        \n        <!-- Suggestions de technologies -->\n        <div class=\"suggestions-section card\">\n          <h3>Technologies à suivre</h3>\n          <div class=\"suggestion-items\">\n            <div class=\"suggestion-item\">\n              <div class=\"suggestion-avatar python\">\n                <i class=\"fab fa-python\"></i>\n              </div>\n              <div class=\"suggestion-info\">\n                <div class=\"suggestion-name\">Python</div>\n                <div class=\"suggestion-handle\">Langage principal</div>\n              </div>\n              <button class=\"btn btn-secondary btn-small\">Suivre</button>\n            </div>\n            <div class=\"suggestion-item\">\n              <div class=\"suggestion-avatar vue\">\n                <i class=\"fab fa-vuejs\"></i>\n              </div>\n              <div class=\"suggestion-info\">\n                <div class=\"suggestion-name\">Vue.js</div>\n                <div class=\"suggestion-handle\">Framework frontend</div>\n              </div>\n              <button class=\"btn btn-secondary btn-small\">Suivre</button>\n            </div>\n            <div class=\"suggestion-item\">\n              <div class=\"suggestion-avatar aws\">\n                <i class=\"fab fa-aws\"></i>\n              </div>\n              <div class=\"suggestion-info\">\n                <div class=\"suggestion-name\">AWS</div>\n                <div class=\"suggestion-handle\">Cloud platform</div>\n              </div>\n              <button class=\"btn btn-secondary btn-small\">Suivre</button>\n            </div>\n          </div>\n          <a href=\"#\" class=\"show-more\">Afficher plus</a>\n        </div>\n        \n        <!-- Contact rapide -->\n        <div class=\"contact-section card\">\n          <h3>Contact rapide</h3>\n          <div class=\"contact-methods\">\n            <a href=\"mailto:amineitji@gmail.com\" class=\"contact-method primary\">\n              <i class=\"fas fa-envelope\"></i>\n              <span>amineitji@gmail.com</span>\n            </a>\n            <a href=\"https://linkedin.com/in/amine-itji-5a8696268\" target=\"_blank\" class=\"contact-method\">\n              <i class=\"fab fa-linkedin\"></i>\n              <span>LinkedIn</span>\n            </a>\n            <a href=\"https://github.com/amineitji\" target=\"_blank\" class=\"contact-method\">\n              <i class=\"fab fa-github\"></i>\n              <span>GitHub</span>\n            </a>\n          </div>\n        </div>\n        \n        <!-- Footer sidebar -->\n        <div class=\"sidebar-footer\">\n          <p>© 2025 Amine ITJI</p>\n          <div class=\"footer-links\">\n            <a href=\"#\">Conditions</a>\n            <a href=\"#\">Confidentialité</a>\n          </div>\n        </div>\n      </aside>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'HomePage',\n  data() {\n    return {\n      activeTab: 'presentation',\n      recentProjects: [\n        {\n          id: 1,\n          title: \"Analyse Football IA\",\n          description: \"Système d'analyse automatique utilisant YOLO et computer vision pour l'analyse de matchs de football en temps réel.\",\n          image: \"https://via.placeholder.com/300x200/1d9bf0/white?text=Football+AI\",\n          category: \"perso\",\n          categoryLabel: \"Personnel\",\n          technologies: [\"Python\", \"YOLO\", \"OpenCV\"],\n          github: \"https://github.com/amineitji/football-ai\"\n        },\n        {\n          id: 2,\n          title: \"DiagERP Platform\",\n          description: \"Plateforme e-commerce complète pour la génération et commercialisation de rapports techniques immobiliers.\",\n          image: \"https://via.placeholder.com/300x200/00ba7c/white?text=DiagERP\",\n          category: \"pro\",\n          categoryLabel: \"Professionnel\",\n          technologies: [\"Django\", \"Vue.js\", \"AWS\"],\n          demo: \"https://diagerp.fr\"\n        },\n        {\n          id: 3,\n          title: \"Web Scraping Analytics\",\n          description: \"Extraction et visualisation automatisée de données de performance de joueurs depuis WhoScored.\",\n          image: \"https://via.placeholder.com/300x200/ffad1f/white?text=Scraping\",\n          category: \"perso\",\n          categoryLabel: \"Personnel\",\n          technologies: [\"Python\", \"Selenium\", \"Data Viz\"],\n          github: \"https://github.com/amineitji/scraping\"\n        }\n      ]\n    };\n  }\n};\n</script>\n\n<style scoped>\n.home-page {\n  background: var(--bg-primary);\n  min-height: 100vh;\n  padding-top: var(--space-4);\n}\n\n/* Profile Header */\n.profile-header {\n  position: relative;\n  border-bottom: 1px solid var(--border-color);\n  padding-bottom: var(--space-6);\n  margin-bottom: var(--space-4);\n}\n\n.cover-photo {\n  height: 200px;\n  background: linear-gradient(135deg, var(--primary) 0%, #0ea5e9 100%);\n  border-radius: var(--radius-lg) var(--radius-lg) 0 0;\n  margin: 0 -var(--space-4) var(--space-4) -var(--space-4);\n}\n\n.profile-info {\n  display: flex;\n  flex-direction: column;\n  gap: var(--space-4);\n}\n\n.avatar-section {\n  position: relative;\n  width: fit-content;\n}\n\n.profile-avatar {\n  width: 120px;\n  height: 120px;\n  border-radius: 50%;\n  border: 4px solid var(--bg-primary);\n  margin-top: -80px;\n  position: relative;\n  z-index: 1;\n}\n\n.status-indicator {\n  position: absolute;\n  bottom: 10px;\n  right: 10px;\n  width: 20px;\n  height: 20px;\n  background: var(--success);\n  border: 3px solid var(--bg-primary);\n  border-radius: 50%;\n  z-index: 2;\n}\n\n.profile-details {\n  flex: 1;\n}\n\n.profile-name {\n  font-size: 1.5rem;\n  font-weight: 800;\n  margin-bottom: var(--space-1);\n}\n\n.profile-handle {\n  color: var(--text-muted);\n  font-size: 15px;\n  margin-bottom: var(--space-2);\n}\n\n.profile-title {\n  font-size: 15px;\n  color: var(--text-secondary);\n  margin-bottom: var(--space-4);\n}\n\n.profile-meta {\n  display: flex;\n  flex-wrap: wrap;\n  gap: var(--space-4);\n  margin-bottom: var(--space-4);\n}\n\n.meta-item {\n  display: flex;\n  align-items: center;\n  gap: var(--space-2);\n  color: var(--text-muted);\n  font-size: 14px;\n}\n\n.meta-item a {\n  color: var(--primary);\n  text-decoration: none;\n}\n\n.profile-stats {\n  