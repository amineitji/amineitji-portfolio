edCategory = '';\n      this.sortBy = 'date';\n      this.expandedProject = null;\n    },\n    getLinkIcon(type) {\n      const icons = {\n        'github': 'ðŸ”—',\n        'demo': 'ðŸš€',\n        'paper': 'ðŸ“„',\n        'docs': 'ðŸ“š',\n        'case-study': 'ðŸ“Š'\n      };\n      return icons[type] || 'ðŸ”—';\n    }\n  }\n};","map":{"version":3,"names":["name","data","expandedProject","selectedCategory","sortBy","projects","title","category","year","imageSrc","technologies","content","links","type","url","label","computed","categories","Set","map","p","sort","filteredProjects","filtered","filter","a","b","localeCompare","methods","toggleProject","index","resetFilters","getLinkIcon","icons"],"sources":["/home/amine/Bureau/PRO/portfolio/amineitji-portfolio/src/views/Projets.vue"],"sourcesContent":["<template>\n  <div class=\"projects-container\">\n    <!-- Effet de particules et d'illumination -->\n    <div class=\"particles-container\">\n      <div class=\"particles\"></div>\n      <div class=\"gradient-overlay\"></div>\n    </div>\n\n    <!-- Filtres et contrÃ´les simplifiÃ©s -->\n    <div class=\"controls-panel animated-entry\">\n      <div class=\"blur-effect purple\" style=\"top: 15%; left: 10%;\"></div>\n      \n      <div class=\"filters-section\">\n        <div class=\"filter-group\">\n          <select v-model=\"selectedCategory\" class=\"filter-select\">\n            <option value=\"\">Toutes les catÃ©gories</option>\n            <option v-for=\"category in categories\" :key=\"category\" :value=\"category\">\n              {{ category }}\n            </option>\n          </select>\n        </div>\n        \n        <div class=\"filter-group\">\n          <select v-model=\"sortBy\" class=\"filter-select\">\n            <option value=\"date\">Date (rÃ©cent d'abord)</option>\n            <option value=\"date-asc\">Date (ancien d'abord)</option>\n            <option value=\"title\">Titre (A-Z)</option>\n            <option value=\"category\">CatÃ©gorie</option>\n          </select>\n        </div>\n        \n        <button @click=\"resetFilters\" class=\"reset-btn\">\n          <span class=\"btn-icon\">âŸ²</span>\n          RÃ©initialiser\n        </button>\n      </div>\n    </div>\n\n    <!-- Timeline des projets -->\n    <div class=\"projects-timeline animated-entry\" style=\"animation-delay: 0.2s;\">\n      <div class=\"blur-effect green\" style=\"bottom: 25%; right: 15%;\"></div>\n      <div class=\"blur-effect cyan\" style=\"top: 60%; left: 60%;\"></div>\n      \n      <div class=\"timeline-line\"></div>\n      \n      <div \n        v-for=\"(project, index) in filteredProjects\" \n        :key=\"index\"\n        class=\"project-card neon-border\"\n        :class=\"{ 'expanded': expandedProject === index }\"\n        @click=\"toggleProject(index)\"\n      >\n        <div class=\"card-header-line\"></div>\n        \n        <!-- Marker temporel -->\n        <div class=\"timeline-marker\">\n          <div class=\"marker-dot\"></div>\n          <div class=\"marker-date\">{{ project.year }}</div>\n        </div>\n        \n        <!-- Contenu principal -->\n        <div class=\"card-content\">\n          <!-- Header compact -->\n          <div class=\"card-header\">\n            <div class=\"header-left\">\n              <h3 class=\"project-title\">{{ project.title }}</h3>\n              <div class=\"project-meta\">\n                <span class=\"category-badge\">{{ project.category }}</span>\n                <span class=\"date-badge\">{{ project.year }}</span>\n              </div>\n            </div>\n            \n            <div class=\"header-right\">\n              <div class=\"tech-stack-preview\">\n                <span \n                  v-for=\"tech in project.technologies.slice(0, 3)\" \n                  :key=\"tech\"\n                  class=\"tech-chip\"\n                >\n                  {{ tech }}\n                </span>\n                <span v-if=\"project.technologies.length > 3\" class=