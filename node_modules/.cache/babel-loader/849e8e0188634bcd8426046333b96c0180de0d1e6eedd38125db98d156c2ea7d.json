{"ast":null,"code":"import \"core-js/modules/es.set.difference.v2.js\";\nimport \"core-js/modules/es.set.intersection.v2.js\";\nimport \"core-js/modules/es.set.is-disjoint-from.v2.js\";\nimport \"core-js/modules/es.set.is-subset-of.v2.js\";\nimport \"core-js/modules/es.set.is-superset-of.v2.js\";\nimport \"core-js/modules/es.set.symmetric-difference.v2.js\";\nimport \"core-js/modules/es.set.union.v2.js\";\nexport default {\n  name: 'ProjectsPage',\n  data() {\n    return {\n      activeFilter: 'all',\n      sortBy: 'date-desc',\n      projects: [{\n        id: 1,\n        title: \"Analyse de matchs de football par IA\",\n        description: `\n            <p>D√©veloppement d'un syst√®me d'analyse automatique utilisant <strong>YOLO</strong> pour la d√©tection de joueurs, <strong>KMeans</strong> pour le clustering par √©quipe, et l'<strong>homographie</strong> pour la projection 2D.</p>\n            <p>üéØ Applications potentielles pour les √©quipes professionnelles et l'analyse tactique avanc√©e.</p>\n          `,\n        category: 'perso',\n        date: '2025-01-15',\n        technologies: ['Python', 'YOLO', 'OpenCV', 'Machine Learning'],\n        media: {\n          type: 'image',\n          url: 'https://via.placeholder.com/600x300/1d9bf0/white?text=Football+AI+Analysis'\n        },\n        github: 'https://github.com/amineitji/football-ai-analysis',\n        likes: 42,\n        comments: 8,\n        shares: 15\n      }, {\n        id: 2,\n        title: \"DiagERP - Plateforme E-commerce Immobilier\",\n        description: `\n            <p><strong>Co-fondateur et d√©veloppeur principal</strong> d'Aneqqis. Plateforme compl√®te pour la g√©n√©ration et commercialisation de rapports techniques immobiliers.</p>\n            <p>üöÄ Architecture Django REST + Vue.js 3 avec d√©ploiement AWS complet</p>\n          `,\n        category: 'pro',\n        date: '2023-05-01',\n        technologies: ['Django', 'Vue.js', 'PostgreSQL', 'AWS', 'Stripe'],\n        media: {\n          type: 'video',\n          url: 'https://www.youtube.com/embed/PiKLRnlXitk'\n        },\n        demo: 'https://diagerp.fr',\n        likes: 67,\n        comments: 12,\n        shares: 23\n      }, {\n        id: 3,\n        title: \"Web Scraping Football Analytics - WhoScored\",\n        description: `\n            <p>Syst√®me automatis√© d'extraction et d'analyse des performances de joueurs depuis WhoScored avec g√©n√©ration de visualisations.</p>\n            <p>üìä Cartes de chaleur automatiques et t√©l√©chargement des photos Transfermarkt</p>\n          `,\n        category: 'perso',\n        date: '2024-06-20',\n        technologies: ['Python', 'Selenium', 'BeautifulSoup', 'Data Viz'],\n        github: 'https://github.com/amineitji/whoscored-scraper',\n        likes: 28,\n        comments: 5,\n        shares: 9\n      }, {\n        id: 4,\n        title: \"Application Web Calcul Empreinte Carbone\",\n        description: `\n            <p>Projet universitaire Master 1 - API REST robuste avec Django et syst√®me d'authentification s√©curis√©.</p>\n            <p>üå± Suite de tests compl√®te et documentation technique d√©taill√©e</p>\n          `,\n        category: 'univ',\n        date: '2024-03-15',\n        technologies: ['Django', 'PostgreSQL', 'REST API', 'Testing'],\n        github: 'https://github.com/amineitji/carbon-tracker',\n        likes: 15,\n        comments: 3,\n        shares: 7\n      }, {\n        id: 5,\n        title: \"Contribution CDlib - Visualisation R√©seaux\",\n        description: `\n            <p>Contribution √† la librairie Python CDlib sous supervision de R√©my Cazabet (UCBL).</p>\n            <p>üîó Nouvelles fonctions de visualisation pour l'analyse de communaut√©s</p>\n          `,\n        category: 'univ',\n        date: '2024-09-10',\n        technologies: ['Python', 'NetworkX', 'Data Visualization'],\n        github: 'https://github.com/GiulioRossetti/cdlib',\n        likes: 22,\n        comments: 4,\n        shares: 6\n      }]\n    };\n  },\n  computed: {\n    filteredProjects() {\n      let filtered = this.projects;\n      if (this.activeFilter !== 'all') {\n        filtered = filtered.filter(project => project.category === this.activeFilter);\n      }\n      switch (this.sortBy) {\n        case 'date-desc':\n          return filtered.sort((a, b) => new Date(b.date) - new Date(a.date));\n        case 'date-asc':\n          return filtered.sort((a, b) => new Date(a.date) - new Date(b.date));\n        case 'title-asc':\n          return filtered.sort((a, b) => a.title.localeCompare(b.title));\n        case 'category':\n          return filtered.sort((a, b) => a.category.localeCompare(b.category));\n        default:\n          return filtered;\n      }\n    },\n    totalProjects() {\n      return this.projects.length;\n    }\n  },\n  methods: {\n    setFilter(filter) {\n      this.activeFilter = filter;\n    },\n    sortProjects() {\n      // Logique dans computed filteredProjects\n    },\n    getProjectCount(category) {\n      return this.projects.filter(p => p.category === category).length;\n    },\n    getCategoryLabel(category) {\n      const labels = {\n        'pro': 'Professionnel',\n        'perso': 'Personnel',\n        'univ': 'Universitaire'\n      };\n      return labels[category] || category;\n    },\n    formatDate(dateString) {\n      const date = new Date(dateString);\n      const now = new Date();\n      const diffTime = Math.abs(now - date);\n      const diffDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24));\n      if (diffDays === 1) return '1j';\n      if (diffDays < 7) return `${diffDays}j`;\n      if (diffDays < 30) return `${Math.ceil(diffDays / 7)}sem`;\n      if (diffDays < 365) return `${Math.ceil(diffDays / 30)}mois`;\n      return date.toLocaleDateString('fr-FR', {\n        year: 'numeric',\n        month: 'short'\n      });\n    },\n    toggleLike(project) {\n      project.liked = !project.liked;\n      project.likes = project.liked ? (project.likes || 0) + 1 : Math.max(0, (project.likes || 0) - 1);\n    },\n    shareProject(project) {\n      if (navigator.share) {\n        navigator.share({\n          title: project.title,\n          url: window.location.href\n        });\n      }\n      project.shares = (project.shares || 0) + 1;\n    },\n    getAllTechnologies() {\n      const allTechs = this.projects.flatMap(p => p.technologies || []);\n      return [...new Set(allTechs)];\n    },\n    getMostUsedTechnologies() {\n      const techCount = {};\n      this.projects.forEach(project => {\n        if (project.technologies) {\n          project.technologies.forEach(tech => {\n            techCount[tech] = (techCount[tech] || 0) + 1;\n          });\n        }\n      });\n      return Object.entries(techCount).map(([name, count]) => ({\n        name,\n        count\n      })).sort((a, b) => b.count - a.count).slice(0, 8);\n    },\n    getActiveYears() {\n      const years = this.projects.map(p => new Date(p.date).getFullYear());\n      return [...new Set(years)].sort((a, b) => b - a);\n    },\n    getProjectsByYear(year) {\n      return this.projects.filter(p => new Date(p.date).getFullYear() === year).length;\n    },\n    filterByYear(year) {\n      console.log('Filter by year:', year);\n    }\n  }\n};","map":{"version":3,"names":["name","data","activeFilter","sortBy","projects","id","title","description","category","date","technologies","media","type","url","github","likes","comments","shares","demo","computed","filteredProjects","filtered","filter","project","sort","a","b","Date","localeCompare","totalProjects","length","methods","setFilter","sortProjects","getProjectCount","p","getCategoryLabel","labels","formatDate","dateString","now","diffTime","Math","abs","diffDays","ceil","toLocaleDateString","year","month","toggleLike","liked","max","shareProject","navigator","share","window","location","href","getAllTechnologies","allTechs","flatMap","Set","getMostUsedTechnologies","techCount","forEach","tech","Object","entries","map","count","slice","getActiveYears","years","getFullYear","getProjectsByYear","filterByYear","console","log"],"sources":["/home/amine/Bureau/PRO/portfolio/amineitji-portfolio/src/views/Projets.vue"],"sourcesContent":["<template>\n  <div class=\"projects-page\">\n    <div class=\"main-layout\">\n      <!-- Sidebar gauche -->\n      <aside class=\"sidebar-left\">\n        <div class=\"filters-card card\">\n          <h3>Filtres</h3>\n          <div class=\"filter-section\">\n            <h4>Cat√©gorie</h4>\n            <div class=\"filter-options\">\n              <button \n                class=\"filter-btn\" \n                :class=\"{ active: activeFilter === 'all' }\"\n                @click=\"setFilter('all')\"\n              >\n                <span class=\"filter-icon\">üìÅ</span>\n                <span>Tous ({{ totalProjects }})</span>\n              </button>\n              <button \n                class=\"filter-btn\" \n                :class=\"{ active: activeFilter === 'pro' }\"\n                @click=\"setFilter('pro')\"\n              >\n                <span class=\"filter-icon\">üíº</span>\n                <span>Professionnel ({{ getProjectCount('pro') }})</span>\n              </button>\n              <button \n                class=\"filter-btn\" \n                :class=\"{ active: activeFilter === 'perso' }\"\n                @click=\"setFilter('perso')\"\n              >\n                <span class=\"filter-icon\">üë§</span>\n                <span>Personnel ({{ getProjectCount('perso') }})</span>\n              </button>\n              <button \n                class=\"filter-btn\" \n                :class=\"{ active: activeFilter === 'univ' }\"\n                @click=\"setFilter('univ')\"\n              >\n                <span class=\"filter-icon\">üéì</span>\n                <span>Universitaire ({{ getProjectCount('univ') }})</span>\n              </button>\n            </div>\n          </div>\n          \n          <div class=\"filter-section\">\n            <h4>Trier par</h4>\n            <select v-model=\"sortBy\" @change=\"sortProjects\" class=\"sort-select\">\n              <option value=\"date-desc\">Plus r√©cent</option>\n              <option value=\"date-asc\">Plus ancien</option>\n              <option value=\"title-asc\">Titre A-Z</option>\n              <option value=\"category\">Cat√©gorie</option>\n            </select>\n          </div>\n        </div>\n        \n        <div class=\"stats-card card\">\n          <h3>Statistiques</h3>\n          <div class=\"stats-grid\">\n            <div class=\"stat-item\">\n              <div class=\"stat-number\">{{ totalProjects }}</div>\n              <div class=\"stat-label\">Projets</div>\n            </div>\n            <div class=\"stat-item\">\n              <div class=\"stat-number\">{{ getAllTechnologies().length }}</div>\n              <div class=\"stat-label\">Technologies</div>\n            </div>\n            <div class=\"stat-item\">\n              <div class=\"stat-number\">{{ getActiveYears().length }}</div>\n              <div class=\"stat-label\">Ann√©es</div>\n            </div>\n          </div>\n        </div>\n      </aside>\n      \n      <!-- Feed principal -->\n      <main class=\"content-column\">\n        <div class=\"page-header\">\n          <div class=\"header-content\">\n            <h1>Mes Projets</h1>\n            <p>{{ filteredProjects.length }} projets trouv√©s</p>\n          </div>\n        </div>\n        \n        <div class=\"projects-feed\">\n          <div class=\"project-tweet\" v-for=\"project in filteredProjects\" :key=\"project.id\">\n            <!-- En-t√™te du tweet -->\n            <div class=\"tweet-header\">\n              <img \n                src=\"https://media.licdn.com/dms/image/v2/D4E03AQE3PrNtqhlw7w/profile-displayphoto-shrink_200_200/profile-displayphoto-shrink_200_200/0/1721847794814?e=2147483647&v=beta&t=-EYtas69BGrXW4KCc708xDpqTbBRHuMtL3f6Oknw4CI\"\n                alt=\"Amine ITJI\"\n                class=\"author-avatar\"\n              />\n              <div class=\"tweet-meta\">\n                <div class=\"author-info\">\n                  <span class=\"author-name\">Amine ITJI</span>\n                  <span class=\"author-handle\">@amineitji</span>\n                  <span class=\"tweet-separator\">¬∑</span>\n                  <span class=\"tweet-date\">{{ formatDate(project.date) }}</span>\n                </div>\n                <div class=\"project-category-badge\" :class=\"'category-' + project.category\">\n                  {{ getCategoryLabel(project.category) }}\n                </div>\n              </div>\n            </div>\n            \n            <!-- Contenu du tweet -->\n            <div class=\"tweet-content\">\n              <h3 class=\"project-title\">{{ project.title }}</h3>\n              <div class=\"project-description\" v-html=\"project.description\"></div>\n              \n              <!-- Technologies -->\n              <div class=\"tech-tags\" v-if=\"project.technologies\">\n                <span v-for=\"tech in project.technologies\" :key=\"tech\" class=\"tech-tag\">\n                  #{{ tech }}\n                </span>\n              </div>\n              \n              <!-- M√©dia du projet -->\n              <div class=\"project-media\" v-if=\"project.media\">\n                <div v-if=\"project.media.type === 'image'\" class=\"media-image\">\n                  <img :src=\"project.media.url\" :alt=\"project.title\" />\n                </div>\n                <div v-else-if=\"project.media.type === 'video'\" class=\"media-video\">\n                  <iframe \n                    :src=\"project.media.url\" \n                    frameborder=\"0\" \n                    allowfullscreen\n                  ></iframe>\n                </div>\n              </div>\n            </div>\n            \n            <!-- Actions du tweet -->\n            <div class=\"tweet-actions\">\n              <button class=\"action-btn\" @click=\"toggleLike(project)\" :class=\"{ liked: project.liked }\">\n                <i class=\"far fa-heart\"></i>\n                <span>{{ project.likes || 0 }}</span>\n              </button>\n              <button class=\"action-btn\">\n                <i class=\"far fa-comment\"></i>\n                <span>{{ project.comments || 0 }}</span>\n              </button>\n              <button class=\"action-btn\" @click=\"shareProject(project)\">\n                <i class=\"fas fa-share\"></i>\n                <span>{{ project.shares || 0 }}</span>\n              </button>\n              \n              <div class=\"external-links\">\n                <a v-if=\"project.github\" :href=\"project.github\" target=\"_blank\" class=\"external-link\">\n                  <i class=\"fab fa-github\"></i>\n                </a>\n                <a v-if=\"project.demo\" :href=\"project.demo\" target=\"_blank\" class=\"external-link\">\n                  <i class=\"fas fa-external-link-alt\"></i>\n                </a>\n              </div>\n            </div>\n          </div>\n        </div>\n      </main>\n      \n      <!-- Sidebar droite -->\n      <aside class=\"sidebar-right\">\n        <div class=\"tech-trending card\">\n          <h3>Technologies populaires</h3>\n          <div class=\"trending-techs\">\n            <div v-for=\"tech in getMostUsedTechnologies()\" :key=\"tech.name\" class=\"trending-tech\">\n              <span class=\"tech-name\">#{{ tech.name }}</span>\n              <span class=\"tech-count\">{{ tech.count }} projets</span>\n            </div>\n          </div>\n        </div>\n        \n        <div class=\"timeline-card card\">\n          <h3>Timeline</h3>\n          <div class=\"timeline-years\">\n            <div v-for=\"year in getActiveYears()\" :key=\"year\" class=\"timeline-year\" @click=\"filterByYear(year)\">\n              <span class=\"year-label\">{{ year }}</span>\n              <span class=\"year-count\">{{ getProjectsByYear(year) }} projets</span>\n            </div>\n          </div>\n        </div>\n      </aside>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'ProjectsPage',\n  data() {\n    return {\n      activeFilter: 'all',\n      sortBy: 'date-desc',\n      projects: [\n        {\n          id: 1,\n          title: \"Analyse de matchs de football par IA\",\n          description: `\n            <p>D√©veloppement d'un syst√®me d'analyse automatique utilisant <strong>YOLO</strong> pour la d√©tection de joueurs, <strong>KMeans</strong> pour le clustering par √©quipe, et l'<strong>homographie</strong> pour la projection 2D.</p>\n            <p>üéØ Applications potentielles pour les √©quipes professionnelles et l'analyse tactique avanc√©e.</p>\n          `,\n          category: 'perso',\n          date: '2025-01-15',\n          technologies: ['Python', 'YOLO', 'OpenCV', 'Machine Learning'],\n          media: {\n            type: 'image',\n            url: 'https://via.placeholder.com/600x300/1d9bf0/white?text=Football+AI+Analysis'\n          },\n          github: 'https://github.com/amineitji/football-ai-analysis',\n          likes: 42,\n          comments: 8,\n          shares: 15\n        },\n        {\n          id: 2,\n          title: \"DiagERP - Plateforme E-commerce Immobilier\",\n          description: `\n            <p><strong>Co-fondateur et d√©veloppeur principal</strong> d'Aneqqis. Plateforme compl√®te pour la g√©n√©ration et commercialisation de rapports techniques immobiliers.</p>\n            <p>üöÄ Architecture Django REST + Vue.js 3 avec d√©ploiement AWS complet</p>\n          `,\n          category: 'pro',\n          date: '2023-05-01',\n          technologies: ['Django', 'Vue.js', 'PostgreSQL', 'AWS', 'Stripe'],\n          media: {\n            type: 'video',\n            url: 'https://www.youtube.com/embed/PiKLRnlXitk'\n          },\n          demo: 'https://diagerp.fr',\n          likes: 67,\n          comments: 12,\n          shares: 23\n        },\n        {\n          id: 3,\n          title: \"Web Scraping Football Analytics - WhoScored\",\n          description: `\n            <p>Syst√®me automatis√© d'extraction et d'analyse des performances de joueurs depuis WhoScored avec g√©n√©ration de visualisations.</p>\n            <p>üìä Cartes de chaleur automatiques et t√©l√©chargement des photos Transfermarkt</p>\n          `,\n          category: 'perso',\n          date: '2024-06-20',\n          technologies: ['Python', 'Selenium', 'BeautifulSoup', 'Data Viz'],\n          github: 'https://github.com/amineitji/whoscored-scraper',\n          likes: 28,\n          comments: 5,\n          shares: 9\n        },\n        {\n          id: 4,\n          title: \"Application Web Calcul Empreinte Carbone\",\n          description: `\n            <p>Projet universitaire Master 1 - API REST robuste avec Django et syst√®me d'authentification s√©curis√©.</p>\n            <p>üå± Suite de tests compl√®te et documentation technique d√©taill√©e</p>\n          `,\n          category: 'univ',\n          date: '2024-03-15',\n          technologies: ['Django', 'PostgreSQL', 'REST API', 'Testing'],\n          github: 'https://github.com/amineitji/carbon-tracker',\n          likes: 15,\n          comments: 3,\n          shares: 7\n        },\n        {\n          id: 5,\n          title: \"Contribution CDlib - Visualisation R√©seaux\",\n          description: `\n            <p>Contribution √† la librairie Python CDlib sous supervision de R√©my Cazabet (UCBL).</p>\n            <p>üîó Nouvelles fonctions de visualisation pour l'analyse de communaut√©s</p>\n          `,\n          category: 'univ',\n          date: '2024-09-10',\n          technologies: ['Python', 'NetworkX', 'Data Visualization'],\n          github: 'https://github.com/GiulioRossetti/cdlib',\n          likes: 22,\n          comments: 4,\n          shares: 6\n        }\n      ]\n    };\n  },\n  computed: {\n    filteredProjects() {\n      let filtered = this.projects;\n      \n      if (this.activeFilter !== 'all') {\n        filtered = filtered.filter(project => project.category === this.activeFilter);\n      }\n      \n      switch (this.sortBy) {\n        case 'date-desc':\n          return filtered.sort((a, b) => new Date(b.date) - new Date(a.date));\n        case 'date-asc':\n          return filtered.sort((a, b) => new Date(a.date) - new Date(b.date));\n        case 'title-asc':\n          return filtered.sort((a, b) => a.title.localeCompare(b.title));\n        case 'category':\n          return filtered.sort((a, b) => a.category.localeCompare(b.category));\n        default:\n          return filtered;\n      }\n    },\n    totalProjects() {\n      return this.projects.length;\n    }\n  },\n  methods: {\n    setFilter(filter) {\n      this.activeFilter = filter;\n    },\n    \n    sortProjects() {\n      // Logique dans computed filteredProjects\n    },\n    \n    getProjectCount(category) {\n      return this.projects.filter(p => p.category === category).length;\n    },\n    \n    getCategoryLabel(category) {\n      const labels = {\n        'pro': 'Professionnel',\n        'perso': 'Personnel', \n        'univ': 'Universitaire'\n      };\n      return labels[category] || category;\n    },\n    \n    formatDate(dateString) {\n      const date = new Date(dateString);\n      const now = new Date();\n      const diffTime = Math.abs(now - date);\n      const diffDays = Math.ceil(diffTime / (1000 * 60 * 60 * 24));\n      \n      if (diffDays === 1) return '1j';\n      if (diffDays < 7) return `${diffDays}j`;\n      if (diffDays < 30) return `${Math.ceil(diffDays / 7)}sem`;\n      if (diffDays < 365) return `${Math.ceil(diffDays / 30)}mois`;\n      \n      return date.toLocaleDateString('fr-FR', { \n        year: 'numeric', \n        month: 'short' \n      });\n    },\n    \n    toggleLike(project) {\n      project.liked = !project.liked;\n      project.likes = project.liked ? (project.likes || 0) + 1 : Math.max(0, (project.likes || 0) - 1);\n    },\n    \n    shareProject(project) {\n      if (navigator.share) {\n        navigator.share({\n          title: project.title,\n          url: window.location.href\n        });\n      }\n      project.shares = (project.shares || 0) + 1;\n    },\n    \n    getAllTechnologies() {\n      const allTechs = this.projects.flatMap(p => p.technologies || []);\n      return [...new Set(allTechs)];\n    },\n    \n    getMostUsedTechnologies() {\n      const techCount = {};\n      this.projects.forEach(project => {\n        if (project.technologies) {\n          project.technologies.forEach(tech => {\n            techCount[tech] = (techCount[tech] || 0) + 1;\n          });\n        }\n      });\n      \n      return Object.entries(techCount)\n        .map(([name, count]) => ({ name, count }))\n        .sort((a, b) => b.count - a.count)\n        .slice(0, 8);\n    },\n    \n    getActiveYears() {\n      const years = this.projects.map(p => new Date(p.date).getFullYear());\n      return [...new Set(years)].sort((a, b) => b - a);\n    },\n    \n    getProjectsByYear(year) {\n      return this.projects.filter(p => new Date(p.date).getFullYear() === year).length;\n    },\n    \n    filterByYear(year) {\n      console.log('Filter by year:', year);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.projects-page {\n  background: var(--bg-primary);\n  min-height: 100vh;\n  padding-top: var(--space-4);\n}\n\n/* Page Header */\n.page-header {\n  padding: var(--space-4);\n  border-bottom: 1px solid var(--border-color);\n  margin-bottom: 0;\n}\n\n.header-content h1 {\n  margin-bottom: var(--space-1);\n}\n\n.header-content p {\n  color: var(--text-muted);\n  font-size: 14px;\n  margin: 0;\n}\n\n/* Sidebar filters */\n.filters-card {\n  margin-bottom: var(--space-4);\n}\n\n.filter-section {\n  margin-bottom: var(--space-4);\n}\n\n.filter-section:last-child {\n  margin-bottom: 0;\n}\n\n.filter-section h4 {\n  font-size: 14px;\n  font-wei